        </div>

        <div class="row content">
          <div class="col-lg-6">
            
            <ul>
              <li><i class="ri-check-double-line"></i>@SharedLanguage["AboutUs_title_left"]</li>            
            </ul>
          </div>
          <div class="col-lg-6 pt-4 pt-lg-0">
            <p>
            @SharedLanguage["AboutUs_title_right"]
