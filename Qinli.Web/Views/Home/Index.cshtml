    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>@SharedLanguage["Contact_titer"]</h2>
          <p>@SharedLanguage["Contact_desc"]</p>
        </div>

        <div>
          <iframe style="border:0; width: 100%; height: 270px;" src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d12097.433213460943!2d-74.0062269!3d40.7101282!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0xb89d1fe6bc499443!2sDowntown+Conference+Center!5e0!3m2!1smk!2sbg!4v1539943755621" frameborder="0" allowfullscreen></iframe>
        </div>

        <div class="row mt-5">

          <div class="col-lg-4">
            <div class="info">
              <div class="address">
                <i class="bi bi-geo-alt"></i>
                <h4>@SharedLanguage["Location"]:</h4>
                <p>May. Deng</p>
              </div>

              <div class="email">
                <i class="bi bi-envelope"></i>
                <h4>@SharedLanguage["Email"]:</h4>
                <p>examle.Qinli.com</p>
              </div>

              <div class="phone">
                <i class="bi bi-phone"></i>
                <h4>@SharedLanguage["Call"]:</h4>
                <p>+86-20-87549341</p>
              </div>

            </div>

          </div>

          <div class="col-lg-8 mt-5 mt-lg-0">

            <form action="" method="post" role="form" class="php-email-form">
              <div class="row">
                <div class="col-md-6 form-group">
                  <input type="text" name="name" class="form-control" id="name" placeholder="@SharedLanguage["Message_Name"]" required>
                </div>
                <div class="col-md-6 form-group mt-3 mt-md-3">
                  <input type="email" class="form-control" name="email" id="email" placeholder="@SharedLanguage["Message_Email"]" required>
                </div>
              </div>
              <div class="form-group mt-3">
                <input type="text" class="form-control" name="subject" id="subject" placeholder="@SharedLanguage["Message_Subject"]" required>
              </div>
              <div class="form-group mt-3">
                <textarea class="form-control" name="message" rows="5" placeholder="@SharedLanguage["Message_Msg"]" required></textarea>
              </div>
            
              <div class="text-center"><button type="submit">@SharedLanguage["Message_btn"]</button></div>
            </form>

          </div>

        </div>

      </div>
    </section><!-- End Contact Section -->
